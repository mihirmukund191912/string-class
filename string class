}
string string::concatstr(){
result=s1+" " + s2;
return result;
}
void strig::comparestr(){
if(s1==s2)
cout<<"both strings are equal"<<endl;
else
cout<<"strings entred are not equal"<<endl;
}
void string::displaystr(){
cout<<"resulting string is:"<<result;
}
int main()
{
